<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    >

    <android.support.v7.widget.AppCompatImageView
        android:id="@+id/wallpaperJSImageView"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_margin="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintVertical_bias="0.0" />


    <ImageButton
        android:id="@+id/somethingButton"
        android:layout_width="@dimen/sphere_icon_small"
        android:layout_height="@dimen/sphere_icon_small"
        android:elevation="6dp"

        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/imageButton1"
        app:layout_constraintBottom_toBottomOf="parent"

        app:layout_constraintHorizontal_chainStyle="packed"
        android:layout_marginBottom="60dp"

        android:src="@drawable/ic_refresh_white_small"
        android:background="@drawable/roundcorner"
        />

    <WebView
        android:id="@+id/webView"
        android:layout_width="1px"
        android:layout_height="1px"
        android:visibility="gone">
    </WebView>

</android.support.constraint.ConstraintLayout>